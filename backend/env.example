# Environment Variables Example
# Esempio Variabili d'Ambiente
# Generated at: 2025-01-27

# Database Configuration / Configurazione Database
DATABASE_URL=jdbc:postgresql://localhost:5432/course_management
DATABASE_USERNAME=postgres
DATABASE_PASSWORD=postgres

# JWT Configuration / Configurazione JWT
JWT_SECRET=mySecretKey123456789012345678901234567890
JWT_EXPIRATION=86400000
JWT_REFRESH_EXPIRATION=604800000

# Mail Configuration / Configurazione Mail
MAIL_HOST=smtp.gmail.com
MAIL_PORT=587
MAIL_USERNAME=your-email@gmail.com
MAIL_PASSWORD=your-app-password
MAIL_SSL_TRUST=smtp.gmail.com

# Redis Configuration / Configurazione Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Server Configuration / Configurazione Server
PORT=8080
CONTEXT_PATH=/
SSL_ENABLED=false
SSL_KEY_STORE=
SSL_KEY_STORE_PASSWORD=
SSL_KEY_STORE_TYPE=PKCS12

# Logging Configuration / Configurazione Logging
LOG_FILE=logs/course-management.log
